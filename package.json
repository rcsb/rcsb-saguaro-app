{
  "name": "@rcsb/rcsb-saguaro-app",
  "version": "5.0.0-data-provider.21",
  "description": "RCSB 1D Saguaro Web App",
  "main": "build/dist/app.js",
  "files": [
    "build/dist"
  ],
  "scripts": {
    "devServer": "webpack-dev-server --config webpack.server.dev.config.js",
    "buildAll": "npm run buildApp && npm run buildAllDoc",
    "buildOnlyApp": "npm run tsc && npm run copyQueries && npm run copyCss && npm run build && npm run copyConfig",
    "buildApp": "npm run cleanAll && npm run checkQueries && npm run buildOnlyApp && npm run clean",
    "buildAppDev": "npm run cleanAll && npm run checkQueries && npm run buildDev",
    "build": "webpack --config ./webpack.config.js",
    "buildDev": "webpack --config ./webpack.server.dev.config.js",
    "checkQueries": "ts-node src/RcsbGraphQL/Queries/QueryCheck.ts",
    "copyQueries": "ncp src/RcsbGraphQL/Queries build/src/RcsbGraphQL/Queries",
    "copyConfig": "ncp build/src/RcsbServerConfig/ServerConfig.js build/dist/config.js",
    "copyCss": "ncp src/RcsbUtils/scss build/src/RcsbUtils/scss && ncp src/RcsbFvWeb/RcsbFvComponents/scss build/src/RcsbFvWeb/RcsbFvComponents/scss  && ncp src/RcsbGroupWeb/RcsbGroupView/RcsbGroupMembers/Components/scss build/src/RcsbGroupWeb/RcsbGroupView/RcsbGroupMembers/Components/scss",
    "tsc": "tsc",
    "clean": "del-cli build/*json build/src",
    "cleanAll": "npm run clean && del-cli build/dist",
    "buildDoc": "typedoc --excludeExternals --externalPattern \"**/node_modules/**\" ./src/app.ts && sed -i '' '/---/d' docs/index.html",
    "cpExamples": "ncp examples docs/examples",
    "buildAllDoc": "npm run buildDoc && npm run cpExamples",
    "publishApp": "npm publish --tag data-provider",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rcsb/rcsb-saguaro-app.git"
  },
  "keywords": [
    "1D",
    "viewer",
    "sequence",
    "annotations",
    "gene",
    "protein",
    "PDB",
    "RCSB",
    "UniProt"
  ],
  "author": "Joan Segura Mora <joan.segura@rcsb.org>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.10",
    "@types/lodash": "^4.14.182",
    "@types/lodash.range": "^3.2.7",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uniqid": "^5.3.2",
    "babel-loader": "^8.2.5",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "del-cli": "^5.0.0",
    "font-awesome": "^4.7.0",
    "graphql-request": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "ideogram": "^1.38.0",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "object-hash": "^3.0.0",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "stream-browserify": "^3.0.0",
    "string-to-color": "^2.2.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.20",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.7.4",
    "uniqid": "^5.4.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@rcsb/rcsb-api-tools": "^4.1.1",
    "@rcsb/rcsb-charts": "^0.0.3",
    "@rcsb/rcsb-saguaro": "^2.5.9",
    "react-select": "^5.4.0",
    "rxjs": "^7.5.5"
  },
  "bugs": {
    "url": "https://github.com/rcsb/rcsb-saguaro-app/issues"
  },
  "homepage": "https://rcsb.github.io/rcsb-saguaro-app",
  "directories": {
    "doc": "docs",
    "example": "examples"
  }
}
